<Window x:Class="WpfCalculadora.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfCalculadora"
        xmlns:y="clr-namespace:WpfCalculadora.Views"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    
    <Grid>
        <y:Login x:Name="LoginForm" Loaded="LoginForm_Loaded" loginCorrect="LoginForm_loginCorrect"></y:Login>
        <y:Calculadora x:Name="CalculadoraForm" Visibility="Hidden"/>
    </Grid>
</Window>
